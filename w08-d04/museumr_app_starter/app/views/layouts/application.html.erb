<!DOCTYPE html>
<html>
<head>
  <title>MuseumrAppStarter</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="home">Museumr</div>
	<div id="container">
		<div class="nav">
			<%= link_to 'Artists', artists_path %>
			<%= link_to 'Paintings', paintings_path %>
			<%= link_to 'Museums', museums_path %>
		</div>

		<%= yield %>

	</div>

</body>
</html>
