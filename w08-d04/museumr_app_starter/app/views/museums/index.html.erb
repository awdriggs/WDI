<div class="nav">
<%= link_to 'New Museum', new_museum_path %>
</div>

<% @museums.each do |museum| %>
<div class="single box">
	<h2><%= link_to museum.name, museum %></h2>
	<%= link_to image_tag( museum.img_url ), museum %>
	<p><%= museum.address %></p>
	 <p><%= museum.description %></p>
</div>
<% end %>